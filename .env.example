# EmailJS Configuration
# Get these values from https://dashboard.emailjs.com/

# Your EmailJS Service ID (e.g., service_21y3wlh)
VITE_EMAILJS_SERVICE_ID=your_service_id_here

# Your EmailJS Template ID (e.g., template_cra_lead)
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here

# Your EmailJS Public Key (found in Account > General)
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Firebase Configuration
# Get these values from Firebase Console > Project Settings > General
# Required for assessment persistence and shareable report links

VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here
